{
  "name": "file-transfer",
  "version": "1.0.0",
  "description": "A simple application that allows someone to anonymously upload a file, and then allow people to download that file with limits on either how long they have to download it, how many times it can be downloaded, or both.",
  "main": "index.js",
  "scripts": {
    "start": "env NODE_ENV=production node server/server.js",
    "build": "env NODE_ENV=production webpack",
    "dev": "env NODE_ENV=development concurrently \"nodemon server/server.js\" \"webpack serve\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
